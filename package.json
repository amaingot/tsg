{
  "name": "tsg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "./node_modules/.bin/webpack --env.prod",
    "fresh": "rm -rf node_modules build reports coverage",
    "lint:ci": "npm run lint -- --format junit --out reports/tslint.xml",
    "lint:fix": "npm run lint -- --fix",
    "lint": "./node_modules/.bin/tslint --project '.'",
    "test:coverage:ci": "npm run test -- --coverage --no-cache --maxWorkers=6 --ci",
    "test:coverage": "npm run test -- --coverage --no-cache --maxWorkers=6",
    "test:ci": "npm run test -- --ci --reporters=default --reporters=jest-junit",
    "test:watch": "npm run test -- --watch",
    "test": "./node_modules/.bin/jest",
    "check": "npm run lint && npm run test:coverage",
    "start": "./node_modules/.bin/webpack-dev-server --env.dev --watch"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.1",
    "@material-ui/icons": "^3.0.2",
    "apollo-client": "^2.4.12",
    "aws-amplify": "^1.1.19",
    "aws-amplify-react": "^2.3.0",
    "aws-appsync": "^1.7.1",
    "aws-appsync-react": "^1.2.6",
    "classnames": "^2.2.6",
    "graphql-tag": "^2.10.1",
    "jss": "^9.8.7",
    "moment": "^2.24.0",
    "perfect-scrollbar": "^1.4.0",
    "react": "^16.7.0",
    "react-apollo": "^2.4.0",
    "react-dom": "^16.7.0",
    "react-popper": "^1.3.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-swipeable-views": "^0.13.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.1.16",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.21",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-swipeable-views": "^0.12.2",
    "@types/sinon": "^7.0.5",
    "@types/webpack": "^4.4.24",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "jest": "^24.0.0",
    "jest-junit": "^6.2.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "prettier": "^1.16.3",
    "raf": "^3.4.1",
    "sinon": "^7.2.3",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": "/src/.*test.(ts|tsx|js)$",
    "setupFilesAfterEnv": [
      "<rootDir>scripts/setupTests.ts"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 0
      }
    }
  }
}
