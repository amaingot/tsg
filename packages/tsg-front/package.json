{
  "name": "tsg-front",
  "version": "1.0.0",
  "description": "Tennis Shop Guru front end TypeScript React App",
  "main": "index.tsx",
  "scripts": {
    "analyze": "./node_modules/.bin/webpack-dev-server --config webpack.analyze.js",
    "build": "./node_modules/.bin/webpack --config webpack.prod.js",
    "fresh": "rm -rf node_modules dist reports",
    "lint:ci": "npm run lint -- --format junit --out reports/tslint.xml",
    "lint:fix": "npm run lint -- --fix",
    "lint": "./node_modules/.bin/tslint {src,test}{,/**,/**/**}/*.{ts,tsx} --config tslint.json",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.dev.js --watch",
    "test:ci": "npm run test -- --ci --reporters=default --reporters=jest-junit",
    "test:coverage:ci": "npm run test -- --coverage --no-cache --maxWorkers=6 --ci",
    "test:coverage": "npm run test -- --coverage --no-cache --maxWorkers=6",
    "test:watch": "npm run test -- --watch",
    "test": "./node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amaingot/tsg-front.git"
  },
  "author": "Alex Maingot",
  "private": true,
  "homepage": "https://github.com/amaingot/tsg-front#readme",
  "dependencies": {
    "@material-ui/core": "^3.7.0",
    "@material-ui/icons": "^3.0.1",
    "@types/classnames": "^2.2.6",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.0.0",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.12",
    "jss": "^9.8.7",
    "jwt-decode": "^2.2.0",
    "moment": "^2.23.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-popper": "^1.3.2",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-swipeable-views": "^0.13.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-saga": "^0.16.2",
    "styled-components": "^4.1.3",
    "typesafe-actions": "^2.0.4"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/history": "^4.7.2",
    "@types/jest": "^23.3.10",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-swipeable-views": "^0.12.2",
    "@types/recompose": "^0.30.2",
    "@types/sinon": "^7.0.2",
    "@types/styled-components": "^4.1.4",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "prettier": "^1.15.3",
    "sinon": "^7.2.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "typescript-styled-plugin": "^0.13.0",
    "webpack": "^4.28.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": "/.*.test.(ts|tsx|js)$",
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.ts",
    "testURL": "http://localhost/",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    }
  }
}
